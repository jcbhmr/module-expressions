{
  "name": "@jcbhmr/esmurl",
  "version": "0.1.0",
  "description": "üéÅ Create a self-contained ESM blob: URL using a function & local import.meta",
  "keywords": [
    "nodejs",
    "javascript",
    "serialization",
    "esm",
    "import-meta",
    "js-functions",
    "importmaps"
  ],
  "license": "MIT",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)",
  "type": "module",
  "imports": {
    "#/esmbody.js": {
      "deno": null,
      "bun": null,
      "node": "./src/esmbody-node.js"
    }
  },
  "exports": {
    ".": {
      "node": "./src/index-node.js",
      "default": "./src/index.js"
    },
    "./esmbody.js": {
      "deno": null,
      "bun": null,
      "node": "./src/esmbody-node.js"
    },
    "./*.js": "./src/*.js",
    "./internal/*": null
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "node prebuild.js",
    "build:docs": "typedoc",
    "docs:dev": "conc \"typedoc --watch\" \"serve docs/dist\"",
    "lint": "prettier --ignore-path .gitignore -w . && tsc --noEmit",
    "test": "node prebuild.js && node --test test/*.test.js",
    "test:watch": "node --test --watch test/*.test.js"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "concurrently": "^8.2.0",
    "import-meta-resolve": "^3.0.0",
    "is-odd": "^3.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "serve": "^14.2.0",
    "terser": "^5.17.4",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.4"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.js"
    ],
    "out": "docs/dist",
    "skipErrorChecking": true
  }
}
