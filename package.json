{
  "name": "@jcbhmr/esmurl",
  "version": "1.0.0",
  "description": "üéÅ Create a self-contained ESM blob: URL using a function & local import.meta",
  "keywords": [
    "nodejs",
    "javascript",
    "serialization",
    "esm",
    "import-meta",
    "js-functions",
    "importmaps"
  ],
  "license": "MIT",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)",
  "type": "module",
  "imports": {
